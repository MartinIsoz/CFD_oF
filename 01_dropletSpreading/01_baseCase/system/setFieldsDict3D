FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.liquid 0
);

regions
(
     sphereToCell
          {
              centre (0 0 0);
              radius 0.001;
              
              fieldValues
              (
                  volScalarFieldValue alpha.liquid 1
              );
          }
);


